<div id="header">
    <div class="v-center" id="sidenav-toggle" onclick="toggleNav()">
        <span class="line"></span>
        <span class="line"></span>
        <span class="line"></span>
    </div>
    <div class="v-center" id = "logo">
        <img src="https://cdn.brawltrack.com/assets/logo.png" />
    </div>
    <div class="v-center" id = "club-selector">
        <select id= "club-selector-tag" onchange="changeClub()">
            <option value=""><%= org %></option>
            <% 
                for (var i = 0; i < clubs.length; ++i){
                let c = clubs[i];%>
                <option value="<%= c[0] %>" <% if (c[0] == club.tag) { %> selected="selected" <% } %>><%= c[1] %></option>
            <% } %>
        </select>
    </div>
</div>
<div id="sidenav" class="sidenav">
    <a href="#">Home</a>
    <a href="#">Blog</a>
    <a href="#">Contact</a>
</div>